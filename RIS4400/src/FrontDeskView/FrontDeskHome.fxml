<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="AnchorPaneMain" prefHeight="803.0" prefWidth="1333.0" stylesheets="@../FrontDesk/FrontDesk.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FrontDesk.FrontDesk">
   <children>
      <TabPane layoutY="-1.0" prefHeight="803.0" prefWidth="1333.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="HomeTab" text="Home">
            <content>
              <AnchorPane fx:id="HomePane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="text-field">
                     <children>
                        <Separator layoutX="522.0" layoutY="-1.0" orientation="VERTICAL" prefHeight="776.0" prefWidth="0.0" />
                        <TableView fx:id="ScheduleViewTable" editable="true" layoutX="523.0" layoutY="-2.0" prefHeight="778.0" prefWidth="810.0">
                          <columns>
                            <TableColumn fx:id="PatientCol" prefWidth="255.0" text="Patient " />
                            <TableColumn fx:id="ProcedureCol" minWidth="0.0" prefWidth="109.0" text="Procedure" />
                              <TableColumn fx:id="TimeCol" minWidth="4.0" prefWidth="83.0" text="Time" />
                              <TableColumn fx:id="PhysCol" prefWidth="242.0" text="Physician" />
                              <TableColumn fx:id="RoomCol" minWidth="8.0" prefWidth="120.0" text="Room No." />
                          </columns>
                           <styleClass>
                              <String fx:value="table-row-cell" />
                              <String fx:value="table-cell" />
                              <String fx:value="text-field-table-cell" />
                           </styleClass>
                           <effect>
                              <DropShadow />
                           </effect>
                        </TableView>
                        <Button fx:id="ScheduleButton" layoutX="105.0" layoutY="263.0" mnemonicParsing="false" text="Schedule Patient" />
                        <Button fx:id="PatientButton" layoutX="335.0" layoutY="263.0" mnemonicParsing="false" onAction="#patientwindow" text="New Patient" />
                        <TextField editable="false" prefHeight="66.0" prefWidth="236.0" text="Welcome">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="welcomeUser" editable="false" layoutX="167.0" layoutY="8.0" prefHeight="51.0" prefWidth="192.0">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="dateBox" editable="false" layoutY="482.0" prefHeight="36.0" prefWidth="201.0">
                           <font>
                              <Font size="14.0" />
                           </font>
                           <effect>
                              <DropShadow />
                           </effect>
                        </TextField>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="ArchiveTab" onSelectionChanged="#loadFromDB" text="Archive">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <HBox layoutX="70.0" layoutY="147.0" prefHeight="26.0" prefWidth="377.0" />
                        <TextArea editable="false" layoutX="-1.0" prefHeight="103.0" prefWidth="518.0" text="Welcome to the archives. Here you can search&#10;for a patient in the table to the right.">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </TextArea>
                        <Separator layoutX="516.0" layoutY="-1.0" orientation="VERTICAL" prefHeight="776.0" prefWidth="0.0" />
                        <TableView fx:id="archiveTable" editable="true" layoutX="516.0" layoutY="-1.0" prefHeight="776.0" prefWidth="818.0">
                          <columns>
                            <TableColumn fx:id="idcol" prefWidth="62.0" text="ID" />
                            <TableColumn fx:id="firstNameCol" prefWidth="114.0" text="First Name" />
                              <TableColumn fx:id="lastNameCol" prefWidth="106.0" text="Last Name" />
                              <TableColumn fx:id="dobcol" prefWidth="64.0" text="DOB" />
                              <TableColumn fx:id="phoneCol" prefWidth="99.0" text="Phone" />
                              <TableColumn fx:id="addrCol" prefWidth="91.0" text="Address 1" />
                              <TableColumn fx:id="addrCol2" prefWidth="94.0" text="Address 2" />
                              <TableColumn fx:id="cityCol" minWidth="0.0" prefWidth="86.0" text="City" />
                              <TableColumn fx:id="stateCol" prefWidth="43.0" text="State" />
                              <TableColumn fx:id="zipCol" prefWidth="58.0" text="Zip" />
                          </columns>
                        </TableView>
                        <TextField alignment="CENTER" editable="false" layoutX="14.0" layoutY="735.0" text="Last edited by:">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="editedByUser" editable="false" layoutX="146.0" layoutY="731.0" prefHeight="30.0" prefWidth="224.0">
                           <font>
                              <Font size="18.0" />
                           </font></TextField>
                        <TextField fx:id="searchTextField" layoutX="170.0" layoutY="242.0" onKeyTyped="#archiveSearch" prefHeight="25.0" prefWidth="177.0" promptText="Search Here">
                           <effect>
                              <InnerShadow />
                           </effect>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                     <styleClass>
                        <String fx:value="table-cell" />
                        <String fx:value="table-row-cell" />
                        <String fx:value="text-field" />
                     </styleClass></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
